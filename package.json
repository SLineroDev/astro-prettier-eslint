		"@typescript-eslint/parser": "^7.2.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-astro": "^0.31.4",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"prettier": "^3.2.5",
		"prettier-plugin-astro": "^0.13.0",
		"typescript": "5.4.2"
